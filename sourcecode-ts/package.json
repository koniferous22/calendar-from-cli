{
  "name": "calendar-from-cli",
  "version": "1.0.0",
  "description": "\"calendar-from-cli\" workspace",
  "repository": "https://github.com/koniferous22/calendar-from-cli",
  "author": "koniferous22",
  "private": true,
  "workspaces": [
    "admin-scripts/*",
    "api/*",
    "cli",
    "jobs/*",
    "packages/*",
    "web"
  ],
  "scripts": {
    "api-admin-ws": "yarn workspace @calendar-from-cli/api-admin",
    "api-admin:dev": "yarn api-admin-ws dev",
    "api-admin:build:docker": "yarn api-admin-ws build:docker",
    "api-admin:build:serverless": "yarn api-admin-ws build:serverless",
    "api-admin:start:docker": "yarn api-admin-ws start:docker",
    "api-admin:deps:watch": "yarn prisma:build && concurrently --names 'validation-lib,calendar-utils,db-queries,backend-lib' --prefix-colors 'yellow,green,magenta,blue' 'yarn:validation-lib:build:watch' 'yarn:calendar-utils:build:watch' 'yarn:db-queries:build:watch' 'yarn:backend-lib:build:watch'",
    "api-public-ws": "yarn workspace @calendar-from-cli/api-public",
    "api-public:dev": "yarn api-admin-ws dev",
    "api-public:build:docker": "yarn api-public-ws build:docker",
    "api-public:build:serverless": "yarn api-public-ws build:serverless",
    "api-public:start:docker": "yarn api-public-ws start:docker",
    "api-public:deps:watch": "yarn prisma:build && concurrently --names 'validation-lib,calendar-utils,db-queries,backend-lib' --prefix-colors 'yellow,green,magenta,blue' 'yarn:validation-lib:build:watch' 'yarn:calendar-utils:build:watch' 'yarn:db-queries:build:watch' 'yarn:backend-lib:build:watch'",
    "aws:build-lambda-layer:libs": "./aws-lambda-layers/scripts/build-lambda-layer-libs",
    "aws:build-lambda-layer:node-modules": "./aws-lambda-layers/scripts/build-lambda-layer-node-modules",
    "aws:build-lambda-layer:prisma": "./aws-lambda-layers/scripts/build-lambda-layer-prisma",
    "backend-lib-ws": "yarn workspace @calendar-from-cli/backend-lib",
    "backend-lib:build": "yarn backend-lib-ws build",
    "backend-lib:build:watch": "yarn backend-lib-ws build:watch",
    "calendar-utils-ws": "yarn workspace @calendar-from-cli/calendar-utils",
    "calendar-utils:build": "yarn calendar-utils-ws build",
    "calendar-utils:build:watch": "yarn calendar-utils-ws build:watch",
    "calendar-utils:test": "yarn calendar-utils-ws test",
    "cli-ws": "yarn workspace @calendar-from-cli/cli-admin",
    "cli:start:dev": "yarn cli-ws start:dev",
    "cli:start:prod": "yarn cli-ws start:prod",
    "common-config-ws": "yarn workspace @calendar-from-cli/common-config",
    "common-config:build": "yarn common-config-ws build",
    "common-config:build:watch": "yarn common-config-ws build:watch",
    "db-queries-ws": "yarn workspace @calendar-from-cli/db-queries",
    "db-queries:build": "yarn db-queries-ws build",
    "db-queries:build:watch": "yarn db-queries-ws build:watch",
    "fmt": "yarn prettier . -u -w",
    "job-calendar-cleanup-ws": "yarn workspace @calendar-from-cli/job-calendar-cleanup",
    "job-calendar-cleanup:build:serverless": "yarn job-calendar-cleanup-ws build:serverless",
    "job-migrate-past-ws": "yarn workspace @calendar-from-cli/job-migrate-past",
    "job-migrate-past:build:serverless": "yarn job-migrate-past-ws build:serverless",
    "job-prepare-environment-ws": "yarn workspace @calendar-from-cli/job-prepare-environment",
    "job-prepare-environment:build:serverless": "yarn job-prepare-environment-ws build:serverless",
    "prisma-ws": "yarn workspace @calendar-from-cli/prisma",
    "prisma:build:local": "yarn prisma-ws build:local",
    "prisma:build:docker": "yarn prisma-ws build:docker",
    "prisma:db:pull:local": "yarn prisma-ws prisma:db:pull:local",
    "prisma:migrate:local": "yarn prisma-ws prisma:migrate:local",
    "prisma:migrate:serverless": "yarn prisma-ws prisma:migrate:serverless",
    "secrets-ws": "yarn workspace @calendar-from-cli/secrets",
    "secrets:build": "yarn secrets-ws build",
    "secrets:build:watch": "yarn secrets-ws build:watch",
    "test": "yarn calendar-utils:test",
    "ui-components-ws": "yarn workspace @calendar-from-cli/ui-components",
    "ui-components:storybook": "yarn ui-components-ws build storybook",
    "ui-components:build": "yarn ui-components-ws build",
    "validation-lib-ws": "yarn workspace @calendar-from-cli/validation-lib",
    "validation-lib:build": "yarn validation-lib-ws build",
    "validation-lib:build:watch": "yarn validation-lib-ws build:watch",
    "web-ws": "yarn workspace @calendar-from-cli/web",
    "web:build:local": "yarn web-ws build:local",
    "web:build:docker": "yarn web-ws build:docker",
    "web:build:serverless": "yarn web-ws build:serverless",
    "web:dev": "yarn web-ws dev",
    "web:preview:local": "yarn web-ws preview:local",
    "web:preview:docker": "yarn web-ws preview:docker"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.1.1"
  }
}
