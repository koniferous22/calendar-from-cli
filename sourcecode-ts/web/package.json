{
  "name": "@calendar-from-cli/web",
  "version": "1.0.0",
  "description": "Calendar CLI base on project admin API",
  "main": "src/index.ts",
  "type": "module",
  "repository": "https://github.com/koniferous22/calendar-from-cli",
  "author": "koniferous22",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "yarn config:prebundle:local && vite",
    "build:local": "tsc && yarn config:prebundle:local && vite build --outDir dist-local",
    "build:docker": "tsc && yarn config:prebundle:docker && vite build --outDir dist-docker",
    "build:serverless": "tsc && yarn config:prebundle:serverless && vite build --outDir dist-serverless",
    "preview:local": "vite preview --host --outDir dist-local",
    "preview:docker": "vite preview --host --outDir dist-docker",
    "config:prebundle:local": "dotenv -e ./env/.env.dev.local tsx scripts/config/generateMergedConfig.ts",
    "config:prebundle:docker": "dotenv -e ./env/.env.dev.docker tsx scripts/config/generateMergedConfig.ts",
    "config:prebundle:serverless": "dotenv -e ./env/.env.prod.serverless tsx scripts/config/generateMergedConfig.ts"
  },
  "dependencies": {
    "@calendar-from-cli/calendar-utils": "*",
    "@calendar-from-cli/common-config": "*",
    "@calendar-from-cli/ui-components": "*",
    "@calendar-from-cli/validation-lib": "*",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "date-fns": "^2",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-router-dom": "^6.22.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@calendar-from-cli/api-public": "",
    "@trpc/server": "^10.45.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "dotenv-cli": "^7.3.0",
    "lodash": "^4.17.21",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
